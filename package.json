{
  "name": "ledger",
  "private": true,
  "version": "0.0.0",
  "main": "main/index.js",
  "scripts": {
    "start": "electron .",
    "start-renderer": "vite",
    "preview": "vite preview",
    "build": "vite build && node ./scripts/afterBuild.js",
    "build:x86": "npm run clean && electron-builder -w --ia32 -c ./build.config.json",
    "build:x64": "npm run clean && electron-builder -w --x64 -c ./build.config.json",
    "build:mac": "npm run clean && electron-builder --mac -c ./build.config.json",
    "clean": "node ./scripts/cleanBuildDir.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ryanpromax/eslint-config": "0.0.4",
    "@vitejs/plugin-react": "^1.0.7",
    "antd": "^4.18.9",
    "array-move": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2",
    "react-sortable-hoc": "^2.0.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "vite": "^2.8.0",
    "vite-plugin-imp": "^2.1.6"
  },
  "dependencies": {
    "dayjs": "^1.10.8",
    "fs-extra": "^10.0.1",
    "lowdb-node": "^3.0.2",
    "uuid": "^8.3.2"
  }
}
