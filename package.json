{
  "name": "electron-react-vite",
  "private": true,
  "version": "0.0.0",
  "main": "main/index.js",
  "scripts": {
    "start": "electron .",
    "start-renderer": "vite",
    "preview": "vite preview",
    "build": "vite build && node ./scripts/afterBuild.js",
    "build:x86": "npm run clean && electron-builder -w --ia32 -c ./build.config.json",
    "build:x64": "npm run clean && electron-builder -w --x64 -c ./build.config.json",
    "build:mac": "npm run clean && electron-builder --mac -c ./build.config.json",
    "clean": "node ./scripts/cleanBuildDir.js"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "cross-env": "^7.0.3",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "fs-extra": "^10.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "vite": "^2.8.0"
  }
}
